<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>true</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>01-start_instance</projectName>
      <filter>*_out</filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.tasks.Shell>
      <command>virtualenv --no-site-packages env
. env/bin/activate
pip install bellatrix --upgrade
bellatrix --version
</command>
    </hudson.tasks.Shell>
    <hudson.plugins.python.Python>
      <command>import os, sys

instance = open(&quot;start_instance_out&quot;).read().split(&quot;,&quot;)[0].strip()
cmd = &quot;. env/bin/activate&quot;
cmd += &quot;&amp;&amp; bellatrix terminate %s&quot; % (instance)
print &quot;executing &quot; + cmd
ret = os.system(cmd)
sys.exit(ret)</command>
    </hudson.plugins.python.Python>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>